{
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "declarationMap": true,
    "inlineSourceMap": true,
    "inlineSources": false,
    "jsx": "react",
    "declaration": true,
    "module": "esnext",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "preserveWatchOutput": true,
    "disableReferencedProjectLoad": true,
    "target": "es2015",
    "lib": ["dom", "es2015"],
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "./lib",
    "composite": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "tsBuildInfoFile": "./lib/tsconfig.tsbuildinfo",
  },
  "include": ["./src"],
  "references": []
}
